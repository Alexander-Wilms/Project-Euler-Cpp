// https://projecteuler.net/problem=31

#include <iostream>
#include <cmath>
#include <string>
#include <sstream>

#define SIZE 1000

template<typename T> ToString(T val) {
	std::stringstream stream;
	stream << val;
	return stream.str();
}

int main() {
	long base = 1;
	long square = 0;
	bool done = false;
	std::string tmp;
	char * tmp_array;

	while(!done) {
		square = pow(base,2);
		tmp = ToString(square);

		std::cout << tmp << " " << sizeof(tmp) << std::endl;
		base++;
	}

	std::cout << base << std::endl;

	return 0;
}
