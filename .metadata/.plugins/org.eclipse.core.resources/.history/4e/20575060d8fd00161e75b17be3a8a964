// https://projecteuler.net/problem=31

#include <iostream>
#include <cmath>
#include <string>
#define SIZE 1000

int main() {
	long base = 1;
	long square = 0;
	bool done = false;
	std::string tmp;
	char * tmp_array;

	while(!done) {
		square = pow(base,2);
		tmp = std::to_string(square);

		std::cout << tmp << " " << sizeof(tmp) << std::endl;
		base++;
	}

	std::cout << base << std::endl;

	return 0;
}
