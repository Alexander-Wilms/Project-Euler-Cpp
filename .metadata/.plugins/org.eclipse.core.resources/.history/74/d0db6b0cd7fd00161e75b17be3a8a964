// https://projecteuler.net/problem=31

#include <iostream>
#include <cmath>
#include <string>
#define SIZE 1000

int main() {
	long base = 1;
	long square = 0;
	bool done = false;
	string tmp;
	char * tmp_array;

	while(!done) {
		square = pow(base,2);
		tmp = to_string(square);

		cout << tmp << " " << sizeof(tmp) << endl;
		base++;
	}

	cout << base << endl;

	return 0;
}
